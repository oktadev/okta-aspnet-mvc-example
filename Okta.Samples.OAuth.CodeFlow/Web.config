<configuration>
  <appSettings>
    <!-- Change these values: -->
    <!-- The Client ID of your Okta application (General - Client Credentials) -->
    <add key="okta:OAuthClientId" value="YourClientIdHere" />
    
    <!-- The Client Secret of your Okta application (General - Client Credentials) -->
    <add key="okta:OauthClientSecret" value="YourClientSecretHere" />
    
    <!-- Your Okta organization URL
         This will always be of the format dev-* (not dev-*-admin)
    -->
    <add key="okta:OAuthAuthority" value="https://dev-12345.oktapreview.com" />
        
    <!-- The scopes that this application will request for the user -->
    <add key="okta:OAuthScopes" value="openid email profile address phone groups offline_access" />
    
    <!-- The OIDC response type to use:
         "code" - the Authorization Code flow
         "code id_token" - the Hybrid flow
    -->
    <add key="okta:OAuthResponseType" value="code id_token" />

    <!-- This application's callback URL (you shouldn't need to change this) -->
    <add key="okta:OAuthRedirectUri" value="https://localhost:44327/Callback" />

    <!-- The URL of the sample resource server (you shouldn't need to change this) -->
    <add key="okta:ApiEndpoint" value="https://localhost:44316/identity" />

    
    <add key="webpages:Version" value="3.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />
  </appSettings>
